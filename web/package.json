{
  "name": "overwatch-web",
  "version": "2.0.0",
  "description": "Web UI for Overwatch monitoring system",
  "main": "index.js",
  "scripts": {
    "dev": "npm run relay && node -r @babel/register server",
    "build": "npm run relay:nowm && next build && babel server --out-dir server.dist",
    "start": "exec env NODE_ENV=production node server.dist",
    "test": "echo \"Error: no test specified\" && exit 1",
    "schema": "get-graphql-schema http://localhost:8485/graphql > schema.graphql",
    "relay": "relay-compiler --src ./ --exclude '**/.next/**' '**/node_modules/**' '**/test/**'  '**/__generated__/**' --schema ./schema.graphql",
    "relay:nowm": "relay-compiler --src ./ --exclude '**/.next/**' '**/node_modules/**' '**/test/**'  '**/__generated__/**' --schema ./schema.graphql --watchman false"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/messa/ow2.git"
  },
  "author": "Petr Messner",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/messa/ow2/issues"
  },
  "homepage": "https://github.com/messa/ow2#readme",
  "dependencies": {
    "express": "^4.16.4",
    "express-session": "^1.16.1",
    "graphql": "^14.2.1",
    "isomorphic-unfetch": "^3.0.0",
    "js-yaml": "^3.13.1",
    "nconf": "^0.10.0",
    "next": "^8.1.0",
    "node-fetch": "^2.5.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-relay": "^3.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/register": "^7.4.4",
    "babel-plugin-relay": "^3.0.0",
    "get-graphql-schema": "^2.1.2",
    "relay-compiler": "^3.0.0"
  }
}
